
{{#myProjectObjects}}

    <a  class="projectSectionImg"
        style="background-image: url('{{dirName}}/screenshot.png');"
        href={{heroku_url}}
        target="_blank"
    >
    </a>
    <div class="textContainer">
        <div class="projectSectionText">
            <a  class="smallContentTitle"
                href={{heroku_url}}
                target="_blank"
            >
                 {{displayName}}
            </a>
            <span class="projectInfoText spanWithSpaceUnder"> {{description}} </span>
            <span class="projectInfoText"> {{tryMsg}} <a href={{heroku_url}} target="_blank" class="linkMobile"> {{heroku_url}} </a></span>
            <span class="projectInfoText"> {{codeMsg}} <a href={{github_url}} target="_blank" class="linkMobile"> {{github_url}} </a></span>
            <span class="projectInfoText"> {{createdWith}} </span>
            <span class="projectInfoTextInline projectInfoText">
                {{comment}}
                <a href={{comment_url}} target="_blank">{{comment_link}}
                </a>
            </span>
        </div>
    </div>

{{/myProjectObjects}}
